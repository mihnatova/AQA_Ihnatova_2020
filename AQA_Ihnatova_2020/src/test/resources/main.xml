<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Ultimate suite" parallel="classes" thread-count="2">
    <listeners>
        <listener class-name="com.AQA_Ihnatova_2020.listeners.TestListener">
        </listener>
    </listeners>
    <test name="Main Tests">
        <groups>
            <run>
                <include name="main"/>
            </run>
        </groups>
        <packages>
            <package name=".*"/>
        </packages>
    </test>
</suite>